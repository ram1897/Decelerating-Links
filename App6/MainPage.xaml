        <Button x:Name="button1_Copy1" DataContext="Main App Function"  Margin="-15,0,0,212"  Foreground ="AntiqueWhite" Tapped="button_click2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="47.5" Width="375" Content="Skip This Tutorial" />

    </Grid>
